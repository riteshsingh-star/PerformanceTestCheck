                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import java.util.regex.Matcher
import java.util.regex.Pattern

// Get the response data as a string
String response = prev.getResponseDataAsString()

// Define regex to extract CSRF token from input field
def pattern = ~/&lt;input[^&gt;]*name=[&apos;&quot;]csrfmiddlewaretoken[&apos;&quot;][^&gt;]*value=[&apos;&quot;]([^&apos;&quot;]+)[&apos;&quot;]/

def matcher = pattern.matcher(response)

if (matcher.find()) {
    String csrfToken = matcher.group(1)
    // Save token into a JMeter variable
    vars.put(&quot;csrf_token&quot;, csrfToken)
    log.info(&quot;Extracted CSRF Token: &quot; + csrfToken)
} else {
    // If token not found, log a warning and set variable to empty or NOT_FOUND
    vars.put(&quot;csrf_token&quot;, &quot;NOT_FOUND&quot;)
    log.warn(&quot;CSRF Token not found in response.&quot;)
}
</stringProp>
